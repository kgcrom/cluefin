{
  "name": "cluefin-openapi",
  "version": "0.1.0",
  "description": "TypeScript OpenAPI client for KIS and Kiwoom",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20",
    "bun": ">=1.3.0"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "generate:metadata": "bun scripts/generate-metadata.mjs",
    "build:types": "bun scripts/generate-types.mjs",
    "build:esm": "bun build ./src/index.ts --outdir ./dist/esm --format esm --target node --packages external",
    "build:cjs": "bun build ./src/index.ts --outdir ./dist/cjs --format cjs --target node --packages external",
    "build": "bun run clean && bun run build:types && bun run build:esm && bun run build:cjs",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "typecheck": "bun scripts/typecheck.mjs",
    "test": "bun test",
    "publish:check": "bun run build && bun run check && bun run typecheck && bun run test",
    "publish:manual": "npm publish --access public"
  },
  "dependencies": {
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/bun": "^1.3.8",
    "typescript": "^5.9.2"
  }
}
