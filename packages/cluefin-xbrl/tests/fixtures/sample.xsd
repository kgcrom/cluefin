<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xbrli="http://www.xbrl.org/2003/instance"
    xmlns:link="http://www.xbrl.org/2003/linkbase"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:sample="http://example.com/sample"
    targetNamespace="http://example.com/sample"
    elementFormDefault="qualified">

    <xsd:import namespace="http://www.xbrl.org/2003/instance"
                schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>

    <xsd:element name="Assets" id="sample_Assets"
                 substitutionGroup="xbrli:item" type="xbrli:monetaryItemType"
                 xbrli:periodType="instant" nillable="true"/>

    <xsd:element name="Equity" id="sample_Equity"
                 substitutionGroup="xbrli:item" type="xbrli:monetaryItemType"
                 xbrli:periodType="instant" nillable="true"/>

    <xsd:element name="Revenue" id="sample_Revenue"
                 substitutionGroup="xbrli:item" type="xbrli:monetaryItemType"
                 xbrli:periodType="duration" nillable="true"/>

</xsd:schema>
